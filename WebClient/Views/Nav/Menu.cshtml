@model IEnumerable<COTES.ISTOK.WebClient.Controllers.NavLink>
@{
    Layout = null;
}

<div id="divmenu">
    <table id="menu">
        <tr>
            @if(Model.Count() > 0)
            {
                foreach (var item in Model)
                {
                    <td><a href="@Url.RouteUrl(item.RouteValues)" class="@(item.IsSelected?"selected":"")">@item.Text</a></td>
                }
            }
            else
            {
                <td>&nbsp;</td>
            }
        </tr>
    </table>
</div>

